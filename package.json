{
  "name": "yen-api",
  "version": "0.1.0",
  "description": "Node.js back-end server",
  "author": "Ihor Andriienko <igorfujitsu@gmail.com>",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "18.17.1"
  },
  "scripts": {
    "prebuild": "yarn clean",
    "build": "nest build",
    "start": "cross-env NODE_ENV=development cross-env NODE_OPTIONS='--enable-source-maps -r ./.pnp.cjs -r dotenv/config' nest start -b swc --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node --enable-source-maps dist/main",
    "clean": "rimraf dist",
    "lint": "concurrently 'yarn lint:source' 'yarn lint:test'",
    "lint:source": "eslint --fix 'src/**/*.ts'",
    "lint:test": "eslint -c .eslintrc.spec.yaml --fix 'test/**/*.spec.ts'",
    "format": "prettier --write {src,test}/**/*.ts",
    "test": "vitest --run --config './test/vite.config.ts'",
    "yen": "cross-env NODE_ENV=development SWCRC=true node -r @swc-node/register ./scripts/cli/index.ts"
  },
